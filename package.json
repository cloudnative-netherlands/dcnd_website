{
  "name": "gatsby-tailwind-starter",
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Alex Barashkov <alex@pixelpoint.io>",
  "engines": {
    "node": ">=20.5.1",
    "npm": ">=v9.8.0"
  },
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 npm run develop",
    "develop": "gatsby develop -H 0.0.0.0",
    "build": "NODE_NO_WARNINGS=1 gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write --ignore-path .gitignore .",
    "lint": "npm run lint:js & npm run lint:md",
    "lint:fix": "npm run lint:js:fix & npm run lint:md:fix",
    "lint:js": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint:js:fix": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --fix --ignore-path .gitignore ."
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fullcalendar/core": "^6.1.11",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/react": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.11",
    "@mdx-js/react": "^2.2.1",
    "@mui/material": "^5.15.17",
    "axios": "^1.7.7",
    "clsx": "^1.2.1",
    "dompurify": "^3.0.3",
    "framer-motion": "^7.6.19",
    "gatsby": "^5.13.7",
    "gatsby-plugin-canonical-urls": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-mdx": "^5.5.0",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.454.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-card-flip": "^1.2.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.3.0",
    "react-social-icons": "^6.14.0",
    "react-tooltip": "^5.28.0",
    "remark-gfm": "^3.0.1",
    "slugify": "^1.6.5",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@svgr/webpack": "^6.5.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-alias-imports": "^1.0.6",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-postcss": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-sitemap": "^6.2.0",
    "gatsby-plugin-svgr-svgo": "^2.0.0",
    "gatsby-source-filesystem": "^5.5.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "postcss": "^8.4.19",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss-safe-area": "^0.2.2"
  },
  "keywords": [
    "react",
    "gatsby",
    "tailwind"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixel-point/gatsby-tailwind-starter"
  },
  "bugs": {
    "url": "https://github.com/pixel-point/gatsby-tailwind-starter/issues"
  }
}
